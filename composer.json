{
    "name": "dewsign/nova-repeater-blocks",
    "description": "Boilerplate repository for creating new Laravel or Javascript/Vue NPM packages",
    "type": "boilerplate",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Mark",
            "email": "m2de@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": ">=5.0.0",
        "michielkempen/nova-polymorphic-field": "^1.0",
        "epartment/nova-dependency-container": "^1.1",
        "laravel/nova": "^1.1",
        "dewsign/nova-field-sortable": "^0.1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": ">=3.1"
    },
    "autoload": {
        "psr-4": {
            "Dewsign\\NovaRepeaterBlocks\\": "src-php/"
        }
    },
    "extra":{
        "laravel": {
            "providers": [
                "Dewsign\\NovaRepeaterBlocks\\Providers\\PackageServiceProvider"
            ]
        }
    }
}
